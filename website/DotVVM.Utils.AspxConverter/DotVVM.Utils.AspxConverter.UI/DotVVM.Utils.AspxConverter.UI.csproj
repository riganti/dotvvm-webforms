<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
  </PropertyGroup>
  <ItemGroup>
    <Content Include="**/*.dothtml;**/*.dotmaster;**/*.dotcontrol" Exclude="obj/**/*.*;bin/**/*.*" CopyToPublishDirectory="Always" />
    <None Remove="**/*.dothtml;**/*.dotmaster;**/*.dotcontrol" />
  </ItemGroup>
  <ItemGroup>
    <Content Remove="compilerconfig.json" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="dotvvm_serialized_config.json.tmp" />
  </ItemGroup>
  <ItemGroup>
    <_ContentIncludedByDefault Remove="wwwroot\css\app.css" />
    <_ContentIncludedByDefault Remove="wwwroot\css\app.min.css" />
  </ItemGroup>
  <ItemGroup>
    <None Include="compilerconfig.json" />
    <None Include="wwwroot\js\syntax\syntax.es5.js" />
    <None Include="wwwroot\js\syntax\syntax.es5.min.js" />
    <None Include="wwwroot\js\syntax\syntax.js" />
    <None Include="wwwroot\js\syntax\syntax_csharp.es5.js" />
    <None Include="wwwroot\js\syntax\syntax_csharp.es5.min.js" />
    <None Include="wwwroot\js\syntax\syntax_csharp.js" />
    <None Include="wwwroot\js\syntax\syntax_dothtml.es5.js" />
    <None Include="wwwroot\js\syntax\syntax_dothtml.es5.min.js" />
    <None Include="wwwroot\js\syntax\syntax_dothtml.js" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="CommonMark.NET" Version="0.15.1" />
    <PackageReference Include="DotVVM.AspNetCore" Version="2.4.0.1" />

  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\DotVVM.Utils.AspxConverter\DotVVM.Utils.AspxConverter.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Compile Update="Resources\Strings.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Strings.resx</DependentUpon>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Update="Resources\Strings.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>Strings.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Controls\" />
  </ItemGroup>
</Project>